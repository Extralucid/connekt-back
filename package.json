{
  "name": "sanlam-api",
  "version": "1.0.0",
  "description": "subscription management app with nodejs, prisma, mongodb, mssql, socket io, swagger, express, jobs etc.",
  "type": "module",
  "main": "server.js",
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "db:seed": "npx prisma db seed",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "db:push": "npx prisma db push",
    "start": "yarn && pm2 start ecosystem.config.cjs",
    "start:prod": "NODE_ENV=production && pm2 start ecosystem.config.cjs",
    "dev": "nodemon src/server.js"
  },
  "repository": "https://gitlab.com/n3v4d4h/mmcop-api.git",
  "author": "maqom",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@prisma/adapter-pg": "^6.0.1",
    "@prisma/client": "^5.19.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@sendgrid/mail": "^8.1.4",
    "@types/node": "^22.5.5",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mmcop-api": "file:",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-rsa": "^1.1.1",
    "pg": "^8.13.1",
    "prisma": "^5.19.1",
    "rate-limit-redis": "^4.2.1",
    "redis": "^5.6.0",
    "socket.io": "^4.8.0",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "uuid": "^10.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "esbuild-register": "^3.6.0",
    "nodemon": "^3.1.7",
    "pm2": "^6.0.8"
  }
}
